{
  "typescript.tsdk": "node_modules/typescript/lib",
  "files": {
    "exclude": {
      ".git": "",
      ".tscache": "",
      "**/*.js": {
        "when": "$(basename).ts"
      },
      "src/**/*.d.ts": true,
      "src/**/*.map": {
        "when": "$(basename)"
      },
      "test/**/*.d.ts": true,
      "test/**/*.map": {
        "when": "$(basename)"
      }
    }
  }
}